{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "graphql": {
      "matchCondition": {
        "route": "/graphql"
      },
      "backendUri": "https://graphitti7.jcb.appserviceenvironment.net/api/build?code=K6yeBiYh2opIYqEwW9yUfyHI2TP8JS23xnPtmndMt8rZQ9rPeI12hA=="
    },
    "static": {
      "matchCondition": {
        "route": "/static/{*restOfPath}"
      },
      "backendUri": "https://graphitti5gpv2.z21.web.core.windows.net/static/{restOfPath}"
    },
    "favicon": {
      "matchCondition": {
        "route": "/favicon.ico"
      },
      "backendUri": "https://graphitti5gpv2.z21.web.core.windows.net/favicon.ico"
    },
    "png": {
      "matchCondition": {
        "route": "/{pngfile}.png"
      },
      "backendUri": "https://graphitti5gpv2.z21.web.core.windows.net/{pngfile}.png"
    },
    "jpg": {
      "matchCondition": {
        "route": "/{jpgfile}.jpg"
      },
      "backendUri": "https://graphitti5gpv2.z21.web.core.windows.net/{jpgfile}.jpg"
    },
    "asset-manifest.json": {
      "matchCondition": {
        "route": "/asset-manifest.json"
      },
      "backendUri": "https://graphitti5gpv2.z21.web.core.windows.net/asset-manifest.json"
    },
    "manifest.json": {
      "matchCondition": {
        "route": "/manifest.json"
      },
      "backendUri": "https://graphitti5gpv2.z21.web.core.windows.net/manifest.json"
    },
    "service-worker": {
      "matchCondition": {
        "route": "/service-worker.js"
      },
      "backendUri": "https://graphitti5gpv2.z21.web.core.windows.net/service-worker.js"
    },
    "precache": {
      "matchCondition": {
        "route": "/precache-manifest.{ext}"
      },
      "backendUri": "https://graphitti5gpv2.z21.web.core.windows.net/precahce-manifest.{ext}"
    },
    "default": {
      "matchCondition": {
        "route": "/{*restOfPath}"
      },
      "backendUri": "https://graphitti5gpv2.z21.web.core.windows.net/index.html"
    }
  }
}
